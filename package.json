{
	"name": "@goker/qr-code",
	"version": "1.0.0",
	"description": "Minimal SVG Output Library (Powered by qr-core)",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"qr",
		"qrcode",
		"svg",
		"generator",
		"matrix",
		"typescript",
		"esm"
	],
	"author": "goker <goker@kodkafa.com>",
	"license": "MIT",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsc",
		"postinstall": "node scripts/patch-qr-core.mjs",
		"prepublishOnly": "npm run build",
		"test": "vitest run --config vitest.config.mjs",
		"lint": "biome check ."
	},
	"dependencies": {
		"qr-core": "^1.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.0",
		"@types/node": "^20.0.0",
		"jsqr": "^1.4.0",
		"nayuki-qr-code-generator": "^1.4.0",
		"pngjs": "^7.0.0",
		"sharp": "^0.33.5",
		"typescript": "^5.3.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=18"
	}
}