{
    "name": "qr-coder",
    "version": "0.0.3",
    "description": "Minimal SVG Output Library (Powered by qr-core)",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "keywords": [
        "qr",
        "qrcode",
        "svg",
        "generator",
        "matrix",
        "typescript",
        "esm"
    ],
    "author": "Goker <goker@kodkafa.com>",
    "license": "MIT",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsc",
        "postinstall": "node scripts/patch-qr-core.mjs",
        "prepublishOnly": "npm run build",
        "test": "vitest run --config vitest.config.mjs",
        "lint": "biome check ."
    },
    "dependencies": {
        "qr-core": "^1.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.5.0",
        "@types/node": "^20.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18"
    }
}